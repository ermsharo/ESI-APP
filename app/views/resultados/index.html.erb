<p id="notice"><%= notice %></p>

<h1>Resultados</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Ingredientes</th>
      <th>Ingredientes tag</th>
      <th>Como fazer</th>
      <th>Imagem</th>
      <th>Origem</th>
      <th>Relevancia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resultados.each do |resultado| %>
      <tr>
        <td><%= resultado.nome %></td>
        <td><%= resultado.ingredientes %></td>
        <td><%= resultado.ingredientes_tag %></td>
        <td><%= resultado.como_fazer %></td>
        <td><%= resultado.imagem %></td>
        <td><%= resultado.origem %></td>
        <td><%= resultado.relevancia %></td>
        <td><%= link_to 'Show', resultado %></td>
        <td><%= link_to 'Edit', edit_resultado_path(resultado) %></td>
        <td><%= link_to 'Destroy', resultado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resultado', new_resultado_path %>
